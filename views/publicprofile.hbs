
{{#if name}}
<h2>{{ name}} profile </h2>

<table class="table">
    <tr>
        <td>Name:</td>
        <td>{{ name }} </td>
    </tr><tr>
        <td>Phone:</td>
        <td>{{ phone }} </td>
    </tr><tr>
        <td>Email:</td>
        <td>{{ email }} </td>
    </tr><tr>
        <td>About:</td>
        <td>{{ About }} </td>
    </tr><tr>
        <td>Age:</td>
        <td>{{ age }} </td>
    </tr><tr>
        <td>Location:</td>
        <td>{{ location }} </td>
    </tr><tr>
        <td>Occupation:</td>
        <td>{{ occupation }} </td>
    </tr><tr>
        <td>Address:</td>
        <td>{{ address }} </td>
    </tr><tr>
        <td>Country:</td>
        <td>{{ country }} </td>
    </tr><tr>
        <td>District:</td>
        <td>{{ district }} </td>
    </tr>
    <tr>
        <td>Gender:</td>
        <td>{{ gender }} </td>
    </tr>
</table>
{{/if}}